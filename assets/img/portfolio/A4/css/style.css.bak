div#modal {
display:none;
z-index:100;
position:fixed;
left:0; top:0;
width:100%; height:100%;
background:#00000080;
}
#modal:target {
display:block;
}
	div.modalwrap {
	position:relative;
	margin:150px auto;
	width:400px; height:300px;
	background:#fff;
	}
		h2.modaltitle {
		width:400px; height:50px;
		text-align:center;
		line-height:48px;
		background:#000;
		color:#fff;
		}
		p.modalcontent {
		margin:30px auto;
		padding:2px 8px;
		width:370px; height:171px;
		background:#fff;
		border:5px solid #000;
		border-radius:15px;
		text-align:justify;
		}
		a.modalclose {
		position:absolute;
		left:337px; top:265px;
		background:#fff;
		color:#000;
		display:block;
		width:60px; height:30px;
		border:2px solid #000;
		border-radius:10px;
		text-align:center;
		line-height:26px;
		}
		a.modalclose:hover {
		background:#fff;
		color:#000;
		font-weight:bold;
		}
