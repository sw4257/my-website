div.slide {
position:relative;
float:left;
width:1200px; height:300px;
overflow:hidden;
}
	div.slide > h2 {
	z-index:20;
	position:absolute;
	left:140px; top:80px;
	width:1000px; height:100px;
	font-size:40px;
	color:#fff;
	text-align:center;
	line-height:100px;
	text-shadow: 2px 2px 4px #000000;
	}
	h2.slidetext1 {
	animation:slidetxt1 9s infinite;
	}
	h2.slidetext2 {
	animation:slidetxt2 9s infinite;
	}
	h2.slidetext3 {
	animation:slidetxt3 9s infinite;
	}
	div.slide > img {
	position:absolute;
	left:0; top:0px;
	}
	img.img1 {
	animation:slide1 9s infinite;
	}
	img.img2 {
	animation:slide2 9s infinite;
	}
	img.img3 {
	animation:slide3 9s infinite;
	}
