		.dropbtn {
		  background-color:#fff;
		  width:200px; height:50px;
		  color:#000;
		  padding: 16px;
		  font-size: 13px;
		  border: none;
		  cursor: pointer;
		}

		.dropdown {
		  position: relative;
		  display:block;
		}

		.dropdown-content {
		  display: none;
		  position: absolute;
		  background-color: #f9f9f9;
		  width: 100px;
		  box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
		  z-index: 1;
		}

		.dropdown-content a {
		  color: black;
		  padding: 20px 20px;
		  text-decoration: none;
		  display: block;
		  width:180px; height:35px;
		}

		.dropdown-content a:hover {background-color:#999}

		.dropdown:hover .dropdown-content {
		  display: block;
		}

		.dropdown:hover .dropbtn {
		  background-color: #999;
		}
@keyframes slide1 {
0% {opacity:1;}
33% {opacity:0;}
66% {opacity:0;}
100% {opacity:1;}
}
@keyframes slide2 {
0% {opacity:0;}
33% {opacity:1;}
66% {opacity:0;}
100% {opacity:0;}
}
@keyframes slide3 {
0% {opacity:0;}
33% {opacity:0;}
66% {opacity:1;}
100% {opacity:0;}
}
@keyframes slidetxt1 {
  0% {opacity:1;left:140px; top:80px;font-size:40px;color:#fff;}
 33% {opacity:0;left:-1200px; top:80px;font-size:10px;color:#000;}
 66% {opacity:0;}
100% {opacity:1;}
}
@keyframes slidetxt2 {
  0% {opacity:0;}
 33% {opacity:1;left:140px; top:80px;font-size:40px;color:#fff;}
 66% {opacity:0;left:1200px; top:80px;font-size:20px;color:#000;}
100% {opacity:0;}
}
@keyframes slidetxt3 {
  0% {opacity:0;}
 33% {opacity:0;}
 66% {opacity:1;left:140px; top:80px;font-size:40px;color:#fff;}
100% {opacity:0;left:140px; top:-100px;font-size:30px;color:#000;}
}
@keyframes svgani {
0% {opacity:1; }
33% {opacity:0; }
66% {opacity:1; }
100% {opacity:0; }
}