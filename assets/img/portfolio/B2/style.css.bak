div#modal {
display:none;
z-index:100;
position:fixed;
left:0; top:0;
width:100%; height:100%;
background:#00000080;
}
#modal:target {
display:block;
}
	div.modalwrap {
	position:relative;
	width:400px; height:300px;
	background:#fef;
	margin:200px auto;
	border:13px solid #aaaafc88;
	border-radius:40px;
	}
		h2.modaltitle {
		width:340px; height:60px;
		font-size:13px;
		text-align:center;
		line-height:58px;
		margin-left:13px;
		}
		p.modalcontents {
		width:340px; height:160px;
		font-size:13px;
		text-align:justify;
		line-height:25px;
		margin-left:13px;
		}
		a.close {
		position:absolute;
		left:240px; top:200px;
		display:block;
		width:100px; height:40px;
		background:#fff;
		color:#000;
		font-size:13px;
		text-align:center;
		line-height:35px;
		border:2px solid #000;
		border-radius:20px;
		margin-top:24px;
		margin-left:5px;
		}
		a.close:hover {
		background:#000;
		color:#fff;
		font-weight:bold;
		border:2px solid #000;
		border-radius:35px;
		}
@keyframes slide1 {
  0% {opacity:1;}
 33% {opacity:0;}
 66% {opacity:0;}
100% {opacity:1;}
}
@keyframes slide2 {
  0% {opacity:0;}
 33% {opacity:1;}
 66% {opacity:0;}
100% {opacity:0;}
}
@keyframes slide3 {
  0% {opacity:0;}
 33% {opacity:0;}
 66% {opacity:1;}
100% {opacity:0;}
}
@keyframes slidetxt1 {
  0% {opacity:1;left:140px; top:80px;font-size:40px;color:#fff;}
 33% {opacity:0;left:-1200px; top:80px;font-size:10px;color:#000;}
 66% {opacity:0;}
100% {opacity:1;}
}
@keyframes slidetxt2 {
  0% {opacity:0;}
 33% {opacity:1;left:140px; top:80px;font-size:40px;color:#fff;}
 66% {opacity:0;left:1200px; top:80px;font-size:20px;color:#000;}
100% {opacity:0;}
}
@keyframes slidetxt3 {
  0% {opacity:0;}
 33% {opacity:0;}
 66% {opacity:1;left:140px; top:80px;font-size:40px;color:#fff;}
100% {opacity:0;left:140px; top:-100px;font-size:30px;color:#000;}
}
@keyframes svgani {
0% {opacity:1; }
33% {opacity:0; }
66% {opacity:1; }
100% {opacity:0; }
}