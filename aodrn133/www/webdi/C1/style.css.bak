@keyframes slideimg1 {
0% {opacity:1; }
33% {opacity:0; }
66% {opacity:0;}
100% {opacity:1; }
}
@keyframes slideimg2 {
0% {opacity:0; }
33% {opacity:1; }
66% {opacity:0;}
100% {opacity:0; }
}
@keyframes slideimg3 {
0% {opacity:0; }
33% {opacity:0; }
66% {opacity:1;}
100% {opacity:0; }
}
@keyframes slidetext1 {
0% {opacity:1; }
33% {opacity:0; }
66% {opacity:0;}
100% {opacity:1; }
}
@keyframes slidetext2 {
0% {opacity:0; }
33% {opacity:1; }
66% {opacity:0;}
100% {opacity:0; }
}
@keyframes slidetext3 {
0% {opacity:0; }
33% {opacity:0; }
66% {opacity:1; }
100% {opacity:0; }
}
@keyframes logoani {
0%	{left:400px; top:200px;}
0.1%{transform: scale(30,30);}
69%	{left:500px; top:300px;}
60%	{transform: scale(0.3,0.3);}
79%	{left:400px; top:200px;}
80%	{transform: scale(3,3);}
81% {transform: rotate(0deg);}
90% {transform: rotate(720deg);}
91%	{left:500px; top:250px;}
99% {transform: scale(1,1);}
100%{left:0px; top:0px;}
}