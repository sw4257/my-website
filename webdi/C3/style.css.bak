@keyframes slidetext1 {
  0% {opacity:1;}
 33% {opacity:0;}
 66% {opacity:0;}
100% {opacity:1;}
}
@keyframes slidetext2 {
  0% {opacity:0;}
 33% {opacity:1;}
 66% {opacity:0;}
100% {opacity:0;}
}
@keyframes slidetext3 {
  0% {opacity:0;}
 33% {opacity:0;}
 66% {opacity:1;}
100% {opacity:0;}
}
@keyframes slide1 {
0%{left:0; top:0;}
33%{left:-800px; top:0; }
34%{left:-800px; top:-300px;}
35%{left:800px; top:-300px;}
66%{left:800px; top:0;}
100%{left:0; top:0; }
}
@keyframes slide2 {
0%{left:800px; top:0; }
33%{left:0; top:0;}
66%{left:-800px; top:0 }
67%{left:-800px; top:-300px;}
68%{left:800px; top:-300px; }
100%{left:800px; top:0;}
}
@keyframes slide3 {
0%{left:-800px; top:-300px;}
1%{left:800px; top:-300px; }
33%{left:800px; top:0; }
66%{left:0; top:0; }
100%{left:-800px; top:0px; }
}
@keyframes svgani {
  0% {transform: rotate(0deg);transform: scale(1,1);}
  5% {transform: scale(1.3,1.3);}
  10% {transform: scale(0.8,0.8);}
  15% {transform: scale(1,1);}
  20% {transform: rotate(3deg);}
  25% {transform: rotate(-3deg);}
  30% {transform: rotate(0deg);}
100% {transform: rotate(0deg);transform: scale(1,1);}
}
@keyframes svgani2 {
  0% {transform: rotate(0deg);transform: scale(1,1);}
  5% {transform: scale(1.3,1.3);}
  10% {transform: scale(0.8,0.8);}
  15% {transform: scale(1,1);}
  20% {transform: rotate(3deg);}
  25% {transform: rotate(-3deg);}
  30% {transform: rotate(0deg);}
100% {transform: rotate(0deg);transform: scale(1,1);}
}