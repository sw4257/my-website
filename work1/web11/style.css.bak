@keyframes slide1 {
0%{left:0; top:0;}
33%{left:-800px; top:0; }
34%{left:-800px; top:-350px;}
35%{left:800px; top:-350px;}
66%{left:800px; top:0;}
100%{left:0; top:0; }
}
@keyframes slide2 {
0%{left:800px; top:0; }
33%{left:0; top:0;}
66%{left:-800px; top:0 }
67%{left:-800px; top:-350px;}
68%{left:800px; top:-350px; }
100%{left:800px; top:0;}
}
@keyframes slide3 {
0%{left:-800px; top:-350px;}
1%{left:800px; top:-350px; }
33%{left:800px; top:0; }
66%{left:0; top:0; }
100%{left:-800px; top:0px; }
}
@keyframes slidetext1 {
  0% {opacity:1;}
 33% {opacity:0;}
 66% {opacity:0;}
100% {opacity:1;}
}
@keyframes slidetext2 {
  0% {opacity:0;}
 33% {opacity:1;}
 66% {opacity:0;}
100% {opacity:0;}
}
@keyframes slidetext3 {
  0% {opacity:0;}
 33% {opacity:0;}
 66% {opacity:1;}
100% {opacity:0;}
}
@keyframes svgani {
0% {opacity:1; }
33% {opacity:0; }
66% {opacity:1; }
100% {opacity:0; }
}