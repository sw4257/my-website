@charset "utf-8";
* {
margin:0; padding:0;
box-sizing:border-box;
}
ul {
list-style-type:none;
}
a {
text-decoration:none;
}
div#modal {
display:none;
z-index:100;
position:fixed;
left:0; top:0;
width:100%; height:100%;
background:#00000080;
}
#modal:target {
display:block;
}
	div.modalwrap {
	position:relative;
	margin:150px auto;
	width:400px; height:300px;
	background:#aaa;
	border:20px solid #dcfeae;
	border-radius:20px;
	}
		h2.modaltitle {
		margin:20px auto;
		width:300px; height:50px;
		background:#fff;
		border:4px solid #000;
		border-radius:30px;
		font-size:20px;
		color:#000;
		text-align:center;
		line-height:40px;
		}	
		p.modalcontent {
		margin:0px auto;
		padding:4px 8px;
		width:340px; height:140px;
		background:#afa;
		border:2px solid #000;
		border-radius:22px;
		font-size:12px;
		color:#000;
		text-align:justify;
		line-height:30px;
		}
		a.modalclose {
		position:absolute;
		left:296px; top:232px;
		display:block;
		width:60px; height:26px;
		background:#fff;
		border:4px solid #000;
		border-radius:20px; 
		font-size:13px;
		color:#000;
		text-align:center;
		line-height:18px;
		}
		a.modalclose:hover {
		background:#fff;
		color:#000;
		font-weight:bold;
		}
div.wrap {
width:1000px; height:650px;
