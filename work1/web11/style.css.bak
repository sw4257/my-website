			ul.mainmenu {

			}
				ul.mainmenu > li {
				border-top:3px solid #000;
				}
				ul.mainmenu > li:last-child {
				border-bottom:3px solid #000;
				}
				ul.mainmenu > li:hover {
				border-top:5px solid #000;
				}
				ul.mainmenu > li:hover > a {
				border-bottom:2px solid #000;
				}
					ul.mainmenu > li > a {
					display:block;
					width:200px; height:50px;
					background:#fff;
					color:#fff;
					text-shadow:1px 1px 0px #000;
					font-size:20px;
					text-align:center;
					line-height:48px;
					}
					ul.mainmenu > li:hover > a{
					background:#000;
					color:#fff;
					text-shadow:2px 2px 0px #000;
					font-weight:bold;
					}
					ul.submenu {
					width:200px; height:0px;
					overflow:hidden;
					transition:all 2s;
					}
					ul.mainmenu > li:hover > ul.submenu {
					height:120px;
					}
						ul.submenu > li {
					
						}
							ul.submenu > li > a {
							display:block;
							width:200px; height:40px;
							background:#fff;
							color:#FF6051;
							font-size:14px;
							text-align:center;
							line-height:38px;
							}
							ul.submenu > li > a:hover {
							background:#fff;
							color:#000;
							font-weight:bold;
							}
